@{
    var parents = new Dictionary<string, Dictionary<string, string>>
{
        {
            "MainPage", new Dictionary<string, string> {{"jsonKey", "Anasayfa"}}
        },
        {
            "HasSub", new Dictionary<string, string> {{"jsonKey", "Form Yönetimi" }}
        },
        {
            "ActivePage", new Dictionary<string, string> {{"jsonKey", "Alan Listesi" }}
        }
    };
}
<main id="js-page-content" role="main" class="page-content">
    <div class="row">
        @await Html.PartialAsync("_Breadcrumb", parents)
    </div>
    <div class="row">
        <div class="col-xl-12">
            <div id="panel-1" class="panel">
                <div class="panel-container show">
                    <div class="panel-content">
                        <div class="row">
                            <div class="col-xl-12">
                                <div class="table-responsive h-scrollbar-primary">
                                    <table id="FormAlanTable" data-topButtons="add" class="table table-bordered table-hover table-striped w-100 nowrap display compact" data-responsive="false">
                                        <thead>
                                            <tr role="row">
                                                <th col-data="Id">ID</th>
                                                <th col-data="Ad" data-orderable="false">Ad</th>
                                                <th col-data="SatirSira">Satır</th>
                                                <th col-data="SutunSira">Sütun</th>
                                                <th col-data="UstId">Üst Id</th>
                                                <th col-role="action" action-list="edit,delete">İşlemler</th>
                                            </tr>
                                        </thead>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>

<div class="page-content-overlay" data-action="toggle" data-class="mobile-nav-on"></div>

<div id="FormAlanModal" aria-hidden="true" aria-labelledby="modalCaption" class="modal fade" data-backdrop="static" data-keyboard="true" role="dialog" tabindex="-1">
    <div class="modal-dialog modal-lg modal-dialog-centered">
        @await Html.PartialAsync("FormAlan")
    </div>
</div>


